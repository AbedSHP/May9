if (`about -api` < 201700){
	warning "May9 Next need Autodesk Maya 2017 and newer to run correctly, please update your Autodesk Maya before install it";
}else{
// Load main Plug-ins
	loadPlugin May9_Next;

	// Auto load main Plug-ins
	pluginInfo -e -a 1 May9_Next;

	// Save Plug-ins auto load prefs
	pluginInfo -spp;

	// Install confimation
	inViewMessage -pos midCenterTop -msg "May9 Next is installed and ready" -fade;
}