
    menuItem
        -l "Show Manips" 
        -c "setToolTo ShowManips" 
        -rp "N" 
        -i "showManip.png" 
        menuEditorMenuItem4;

    menuItem
        -l "Edit Attributes" 
        -c "AttributeEditor" 
        -rp "S" 
        -i "attributes.png" 
        menuEditorMenuItem3;

    menuItem
        -l "Chamfer On" 
        -c "string $myBevel[] = `ls -sl-typ \"polyBevel3\" -tl 1`; setAttr ($myBevel[0] + \".chamfer\") 1;" 
        -i "rvViewingPipelineOn.png" 
        menuEditorMenuItem1;

    menuItem
        -l "Chamfer Off" 
        -c "string $myBevel[] = `ls -sl-typ \"polyBevel3\" -tl 1`; setAttr ($myBevel[0] + \".chamfer\") 0;" 
        -i "rvViewingPipelineOff.png" 
        menuEditorMenuItem2;

setParent -m ..;

