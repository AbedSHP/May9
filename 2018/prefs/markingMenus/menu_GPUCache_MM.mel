
    menuItem
        -label "Make Live" 
        -command "string $mySel[] = `ls -sl`;\nMakeLive;\nselect $mySel;" 
        -radialPosition "N" 
        -image "makeLive.png" 
        -sourceType "mel" 
        menuEditorMenuItem2;

    menuItem
