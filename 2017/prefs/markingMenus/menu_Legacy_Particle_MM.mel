int $particleCountCheck = `optionVar -q particleCountVisibility`;

    menuItem
        -label "Use as Fluid Emitter" 
        -command "dynExecFluidEmitterCommands 1 { \"0\", \"fluidEmitter -type surface -der 1 -her 1 -fer 1 -fdr 2 -r 100.0 -cye none -cyi 1 -mxd 1 -mnd 0 \", 0, 0, 0, 0, 0, 0, 10, 10, 10, 10, 10, 10, 0, 0, 0} " 
        -radialPosition "NW" 
        -image "fluidObjectEmit.png" 
        -sourceType "mel" 
        menuEditorMenuItem2;

    menuItem
        -label "menuEditorMenuItem4" 
        -command "EmitFluidFromObjectOptions" 
        -optionBox 1
        -sourceType "mel" 
        menuEditorMenuItem4;

    menuItem
        -label "Lattice" 
        -command "CreateLattice" 
        -radialPosition "NE" 
        -image "lattice.png" 
        -sourceType "mel" 
        menuEditorMenuItem5;

    menuItem
        -label "menuEditorMenuItem7" 
        -command "CreateLatticeOptions;" 
        -optionBox 1
        -sourceType "mel" 
        menuEditorMenuItem7;

    menuItem
        -label "Cluster" 
